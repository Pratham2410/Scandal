<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonViewMyEvents_OrganisorHomepage"
        style="@style/btnSolidRounded"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/_47pxv"
        android:layout_gravity="center_horizontal"
        android:ellipsize="end"
        android:gravity="center"
        android:paddingStart="@dimen/_29pxh"
        android:paddingTop="@dimen/_10pxv"
        android:paddingEnd="@dimen/_28pxh"
        android:paddingBottom="@dimen/_10pxv"
        android:singleLine="true"
        android:text="View My Events"
        android:textAllCaps="false"
        app:layout_constraintBottom_toTopOf="@+id/sendnotifs"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonCreateNewEvents"
        tools:text="View My Events" />

    <TextView
        android:id="@+id/textPressBelow1"
        style="@style/txtPoppinsbold16"
        android:layout_width="match_parent"
        android:layout_height="32dp"
        android:ellipsize="end"
        android:gravity="center_horizontal"
        android:singleLine="true"
        android:text="Press below to make a new event!"
        app:layout_constraintBottom_toTopOf="@+id/buttonCreateNewEvents"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameStackellipseone"
        tools:text="Press below to make a new event!" />

    <LinearLayout
        android:id="@+id/buttonCreateNewEvents"
        style="@style/groupStyleteal_1"
        android:layout_width="259dp"
        android:layout_height="48dp"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/buttonViewMyEvents_OrganisorHomepage"
        app:layout_constraintTop_toBottomOf="@+id/textPressBelow1">

        <TextView
            android:id="@+id/textOrganisorPage"
            style="@style/txtPoppinsbold17"
            android:layout_width="204dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_11pxv"
            android:layout_marginBottom="@dimen/_9pxv"
            android:layout_weight="0.83"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="Create New Event"
            tools:text="Create New Event" />

    </LinearLayout>

    <FrameLayout
        android:id="@+id/frameStackellipseone"
        android:layout_width="411dp"
        android:layout_height="290dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageUndrawdoneche"
            android:layout_width="146dp"
            android:layout_height="@dimen/_152pxh"
            android:layout_gravity="top|start"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginBottom="@dimen/_10pxv"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/img_undrawdoneche"
            tools:ignore="ContentDescription"
            tools:src="@drawable/img_undrawdoneche" />

        <FrameLayout
            android:id="@+id/buttonBack_OrganisorHomepage"
            style="@style/groupStyleteal_A200cornerRadius"
            android:layout_width="@dimen/_82pxh"
            android:layout_height="@dimen/_51pxv"
            android:layout_marginEnd="@dimen/_10pxh">

            <ImageView
                android:id="@+id/imageArrowleft"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_25pxh"
                android:layout_gravity="top|start"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_marginEnd="@dimen/_26pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowleft"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_arrowleft" />
        </FrameLayout>

        <ImageView
            android:id="@+id/profilePicture"
            android:layout_width="@dimen/_153pxh"
            android:layout_height="@dimen/_112pxh"
            android:layout_gravity="top|end"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginEnd="@dimen/_8pxh"
            android:layout_marginBottom="@dimen/_10pxv"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/img_ellipse1"
            tools:ignore="ContentDescription"
            tools:src="@drawable/img_ellipse1" />

        <ImageView
            android:id="@+id/imageEllipseTwo"
            android:layout_width="@dimen/_97pxh"
            android:layout_height="@dimen/_153pxh"
            android:layout_gravity="top|end"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_marginBottom="@dimen/_13pxv"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/img_ellipse2"
            tools:ignore="ContentDescription"
            tools:src="@drawable/img_ellipse2" />

        <TextView
            android:id="@+id/txtWelcome"
            style="@style/txtPoppinsbold20"
            android:layout_width="408dp"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center"
            android:layout_marginStart="@dimen/_67pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_marginEnd="@dimen/_67pxh"
            android:ellipsize="end"
            android:gravity="center"
            android:singleLine="true"
            android:text="Welcome to Organisor's Page!"
            android:textSize="24sp"
            tools:text="Welcome to Organisor's Page!" />

    </FrameLayout>

    <View
        android:id="@+id/viewEllipseFour"
        android:layout_width="@dimen/_153pxh"
        android:layout_height="@dimen/_153pxh"
        android:layout_gravity="bottom|end"
        android:layout_marginStart="4dp"
        android:background="@drawable/rectangle_bg_teal_a200_77_radius_76_5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageEllipseThree"
        android:layout_width="119dp"
        android:layout_height="163dp"
        android:layout_gravity="top|start"
        android:layout_marginStart="4dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_ellipse3"
        tools:ignore="ContentDescription"
        tools:src="@drawable/img_ellipse3" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="165dp"
        android:layout_height="208dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/img_completed_m9ci" />

    <FrameLayout
        android:id="@+id/buttonBackToHomepage"
        android:layout_width="@dimen/_89pxh"
        android:layout_height="@dimen/_90pxv"
        android:layout_gravity="bottom|start"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/imageLogo"
            android:layout_width="@dimen/_89pxh"
            android:layout_height="@dimen/_90pxh"
            android:layout_gravity="start"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/img_result1"
            tools:ignore="ContentDescription"
            tools:src="@drawable/img_result1" />

        <TextView
            android:id="@+id/txtHomePage"
            style="@style/txtPoppinsbold15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="top|start"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginBottom="@dimen/_16pxv"
            android:gravity="top|center"
            android:shadowColor="#65FEEB"
            android:shadowDx="1.5"
            android:shadowDy="1.3"
            android:shadowRadius="2.5"
            android:text="Home Page"
            android:textAllCaps="true"
            tools:text="HOME PAGE" />
    </FrameLayout>

    <Button
        android:id="@+id/sendnotifs"
        style="@style/txtPoppinsbold17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:shadowColor="#FFC107"
        android:text="Send Notifications"
        android:textColor="@color/pink_500"
        android:textColorHighlight="#FFC107"
        android:textColorHint="#FFC107"
        android:textColorLink="#FFC107"
        app:layout_constraintBottom_toTopOf="@+id/buttonBackToHomepage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.426"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonViewMyEvents_OrganisorHomepage"
        app:rippleColor="#FFC107"
        app:strokeColor="#FFC107" />

</androidx.constraintlayout.widget.ConstraintLayout>