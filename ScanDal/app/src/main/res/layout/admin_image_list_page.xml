<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonView_AdminImageListPage"
        style="@style/btnSolidRoundedOutline"
        android:layout_width="180dp"
        android:layout_height="52dp"
        android:layout_gravity="top|center"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="16dp"
        android:ellipsize="end"
        android:gravity="center"
        android:paddingStart="@dimen/_30pxh"
        android:paddingTop="@dimen/_8pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingBottom="@dimen/_8pxv"
        android:singleLine="true"
        android:text="View"
        android:textAllCaps="false"
        android:textSize="17sp"
        app:layout_constraintBottom_toTopOf="@+id/buttonBackToHomepage"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listView_ImageListPage"
        app:layout_constraintVertical_bias="0.59"
        tools:text="View" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonDelete_AdminImageListPage"
        style="@style/btnSolidRoundedOutline"
        android:layout_width="180dp"
        android:layout_height="52dp"
        android:layout_gravity="top|center"
        android:layout_marginEnd="16dp"
        android:ellipsize="end"
        android:gravity="center"
        android:paddingStart="@dimen/_30pxh"
        android:paddingTop="@dimen/_8pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingBottom="@dimen/_8pxv"
        android:singleLine="true"
        android:text="Delete"
        android:textAllCaps="false"
        android:textSize="17sp"
        app:layout_constraintBottom_toTopOf="@+id/buttonBackToHomepage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listView_ImageListPage"
        app:layout_constraintVertical_bias="0.172"
        tools:text="Delete" />

    <ListView
        android:id="@+id/listView_ImageListPage"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/buttonView_AdminImageListPage"
        app:layout_constraintTop_toBottomOf="@+id/txtImageList" />

    <ImageView
        android:id="@+id/imageUndrawcomplete"
        android:layout_width="148dp"
        android:layout_height="196dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/img_undrawdoneche"
        tools:ignore="ContentDescription"
        tools:src="@drawable/img_undrawdoneche" />

    <FrameLayout
        android:id="@+id/buttonBackToHomepage"
        android:layout_width="@dimen/_89pxh"
        android:layout_height="@dimen/_90pxv"
        android:layout_gravity="bottom|start"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/imageLogo"
            android:layout_width="@dimen/_89pxh"
            android:layout_height="@dimen/_90pxh"
            android:layout_gravity="start"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/img_result1"
            tools:ignore="ContentDescription"
            tools:src="@drawable/img_result1" />

    </FrameLayout>

    <TextView
        android:id="@+id/txtImageList"
        style="@style/txtPoetsenOneregular26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="25dp"
        android:ellipsize="end"
        android:singleLine="true"
        android:text="Image List"
        app:layout_constraintBottom_toTopOf="@+id/listView_ImageListPage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearRowarrowleft"
        tools:text="Image List" />

    <LinearLayout
        android:id="@+id/linearRowarrowleft"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <FrameLayout
            android:id="@+id/buttonBack_ImageListPage"
            style="@style/groupStyleteal_A200cornerRadius"
            android:layout_width="@dimen/_82pxh"
            android:layout_height="@dimen/_51pxv">

            <ImageView
                android:id="@+id/imageArrowleft"
                android:layout_width="@dimen/_23pxh"
                android:layout_height="@dimen/_25pxh"
                android:layout_gravity="top|start"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_marginEnd="@dimen/_26pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowleft"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_arrowleft" />
        </FrameLayout>

        <ImageView
            android:id="@+id/imageGearOne"
            android:layout_width="@dimen/_41pxh"
            android:layout_height="@dimen/_41pxh"
            android:layout_marginStart="@dimen/_230pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:scaleType="fitXY"
            android:src="@drawable/img_gear1"
            tools:ignore="ContentDescription"
            tools:src="@drawable/img_gear1" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
